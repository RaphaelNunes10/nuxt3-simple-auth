{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint-watch": "onchange \"**/*.ts\" \"**/*.js\" \"**/*.vue\" -- eslint --fix {{changed}}",
